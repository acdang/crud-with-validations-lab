<h1>All Songs</h1>

<table>
    <thead>
        <tr>
            <th>Title</th>
            <th>Year</th>
        </tr>
    </thead>
    <tbody>
        <% @songs.each do |song| %>
            <tr>
                <td><%= song.title %></td>
                <td><%= song.release_year %></td>
            </tr>
        <% end %>
    </tbody>
</table>
